# Frontend Staging Environment
# Copy this to .env for staging deployment

# Backend API URL for staging
REACT_APP_API_URL=https://your-staging-backend.herokuapp.com/api

# App Configuration
REACT_APP_NAME=Fitness Tracker
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=staging

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_CONSOLE_LOGS=true

# Staging Settings
REACT_APP_MOCK_API=false
REACT_APP_USE_LOCAL_STORAGE=true
